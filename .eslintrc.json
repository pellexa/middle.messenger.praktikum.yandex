{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [2, "never"],
    "max-len": [2, 100],
    "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "ts": "never"
        }
     ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
} 
